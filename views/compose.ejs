<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="mb-3">
   <label for="inputTitle" class="form-label">Title</label>
   <input type="text" name="postTitle" class="form-control" id="inputTitle" aria-describedby="titleEntry">
   <!-- <div id="titleEntry" class="form-text">Enter Title for your post.</div> -->
 </div>
 <div class="mb-3">
  <label for="inputPost" class="form-label">Post</label>
  <textarea type="text" name="postBody" rows="8" cols="80" class="form-control" id="inputPost" aria-describedby="postEntry"></textarea>
  <!-- <div id="postEntry" class="form-text">Enter Post</div> -->
</div>
<br />

  <button class="btn btn-primary" type="submit" name="composeBtn">Publish</button>
</form>




<%- include("partials/footer") -%>
